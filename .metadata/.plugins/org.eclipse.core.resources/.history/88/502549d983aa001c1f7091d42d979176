
public class ThisKeyword {

	int a=9;
	 
	ThisKeyword() //Constructor
	{
		System.out.println(this); //this keyword is used to point to object members
		System.out.println(this.a);
		System.out.println(a);
	}
	
	
	public static void main(String[] args) {
		
		new ThisKeyword();
		new ThisKeyword();
		
	}
}
